<tbody>
    <%for(var i = 0; i < dulieu.length;i++){%>
    <tr>
        <th scope="row"><%=i+1%></th>
        <td><img src="<%=dulieu[i].hinhanh%>" width="80px" height="80px" alt=""></td>
        <td>
            <a href="/sp/chitiet/<%=dulieu[i].id%>" class="text-dark fw-bold"><%=dulieu[i].tensanpham%></a>
        </td>
        <td><%=dulieu[i].tenloaisanpham%></td>
        <td>
            <p><%=dulieu[i].mota%></p>
        </td>
        <td><%=dulieu[i].giasanpham%></td>
        <td><%=dulieu[i].diachi%></td>
        <td class="action">
            <a href="/sp/upPro/<%=dulieu[i].id%>" class="btn border-shadow update">
                <span class="text-gradient">
                    <i class="fa-solid fa-pen-to-square"></i>
                </span>
            </a>

            <a href="" class="btn border-shadow delete" id="delete" data-id="<%=dulieu[i].id%>">
                <span class="text-gradient">
                    <i class="fa-solid fa-trash"></i>
                </span>
            </a>
        </td>
    </tr>

    <%}%>
</tbody>